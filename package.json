{
  "name": "tree-sitter-xresources",
  "version": "0.1.0",
  "description": "Xresources grammar for tree-sitter",
  "repository": "github:tree-sitter/tree-sitter-xresources",
  "license": "MIT",
  "author": {
    "name": "Omar Valdez",
    "email": "omarantoniovaldezf2@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "type": "module",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "xresources"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "parse": "tree-sitter parse --quiet --stat examples/*"
  }
}
