{
  "name": "tree-sitter-xresources",
  "version": "0.0.1",
  "description": "Xresources grammar for tree-sitter",
  "repository": "github:ValdezFOmar/tree-sitter-xresources",
  "author": "Omar Valdez <omarantoniovaldezf2@gmail.com>",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "type": "module",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "xresources"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-cli": "^0.23.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.xresources",
      "highlights": "queries/highlights.scm",
      "injection-regex": "^xresources$",
      "file-types": [
        "xdefaults",
        "xresources",
        "Xdefaults",
        "Xresources",
        ".Xdefaults",
        ".Xresources"
      ]
    }
  ]
}
